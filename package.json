{
  "name": "cdk-es-on-ec2",
  "private": true,
  "packageManager": "pnpm@10.25.0",
  "scripts": {
    "build": "pnpm -r --filter \"./packages/**\" build",
    "watch": "pnpm -r --filter \"./packages/**\" watch",
    "test": "pnpm -r --filter \"./packages/**\" test",
    "test:update": "pnpm -r --filter \"./packages/**\" test:update",
    "check": "pnpm -r --filter \"./packages/**\" check",
    "diff": "pnpm -r --filter \"./packages/**\" diff",
    "deploy": "pnpm -r --filter \"./packages/**\" deploy",
    "multi:check": "pnpm -C packages/multi run check",
    "multi:diff": "pnpm -C packages/multi run diff",
    "multi:deploy": "pnpm -C packages/multi run deploy",
    "multi:apply": "pnpm -C packages/multi run apply",
    "single:check": "pnpm -C packages/single run check",
    "single:diff": "pnpm -C packages/single run diff",
    "single:deploy": "pnpm -C packages/single run deploy"
  }
}
